FILENAME = ./Reports/report_labs

$(FILENAME).pdf: $(FILENAME).tex

#compiling twice for autogenerated lists
	-pdflatex -interaction=nonstopmode $(FILENAME).tex
	-pdflatex -interaction=nonstopmode $(FILENAME).tex

#removing autogenerated stuff
	rm -f *.aux
	rm -f *.log
	rm -f *.lof
	rm -f *.lot
	rm -f *.out
	rm -f *.toc
	rm -f *.synctex.gz

exams:
	-pdflatex -interaction=nonstopmode ./Reports/Exams2014-summary.tex
	-pdflatex -interaction=nonstopmode ./Reports/Exams2014-summary.tex

	rm -f *.aux
	rm -f *.log
	rm -f *.lof
	rm -f *.lot
	rm -f *.out
	rm -f *.toc
	rm -f *.synctex.gz

clean:
	rm -f report_labs.pdf
	rm -f Exams2014-summary.pdf

.PHONY: exams, clean
